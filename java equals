public class Circle {
	private int x, y, radius;
	public Circle(int x, int y, int radius)
	{
		this.x = x;
		this.y = y;
		this.radius = radius;
		
	}
	public boolean equals(Circle obj)
	{
		if(this.radius == obj.radius)
			return true;
		else 
			return false;
	}
