import java.util.Scanner;public class CircleTest { public static void main(String[] args) {    Scanner input = new Scanner(System.in); //입력받는 모든 문자를 처리해줌             //Scanner클래스가.  System.out.println("x값?");  int x = input.nextInt();  System.out.println("y값?");  int y = input.nextInt();  System.out.println("radius값?");  int radius = input.nextInt();    Circle c1 = new Circle(x,y,radius);    System.out.println("x값?");  x = input.nextInt();  System.out.println("y값?");  y = input.nextInt();  System.out.println("radius값?");  radius = input.nextInt();    Circle c2 = new Circle(x,y,radius);      //(c1.equals(c2)) ? (c2.equals(c2)) : ()  System.out.println(c1.equals(c2));  }         }
