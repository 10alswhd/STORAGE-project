import java.util.Scanner;public class Circle { private int x; private int y; private int radius; public Circle(int x,int y, int radius) {  this.x = x;  this.y = y;  this.radius = radius; }  public boolean equals(Circle c){  if(c.radius == radius)  {   return true;  }     return false;  }    public static void main(String[] args) {   Circle a = new Circle(1,2,3);   Circle b = new Circle(5,6,3);   if(a.equals(b)){    System.out.println("반지름의 길이는 서로 같습니다.");   }               }  }
