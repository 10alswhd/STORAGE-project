#include<stdio.h> #include<stdlib.h>int main(void) {    int i = 1;   int j = 0;    int temp = 0;   int sel1 = 0;   printf("숫자를 입력하세요 : ");   scanf("%d",&j);   printf("원하는 형식을 고르세요 :\n");    printf("1.while : \n");      printf("2.for : \n");    printf("3.do-while : \n");   scanf("%d", &sel1);                  if(sel1 == 1)    {       while(i <= 9)    {    temp = j * i;    printf("%d단 => %d x %d = %d\n", j , j , i  , temp);    i++;    }   }           else if(sel1 == 2)       {   for(i= 1; i<= 9 ; i++)    {    temp = j * i;    printf("%d단 => %d x %d = %d\n", j , j , i  , temp);        }    }            else if(sel1 == 3)     {          i = 1;       do{                    temp = j * i;     printf("%d단 => %d x %d = %d\n", j , j , i  , temp);      i++;                   }while(i<=9);       }      else printf("잘못입력하셨습니다.~!!!!!\n");    system("pause");}
