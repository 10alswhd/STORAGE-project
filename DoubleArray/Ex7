#include <stdio.h>#include <stdlib.h>void main(){   int i, j;   int nRow=5, nCol=4;   int **aNum;// 유동 할당하기   aNum = (int **)malloc(nRow * sizeof(int *));   for(i=0; i<nRow; ++i)   {        aNum[i] = (int *)malloc( nCol * sizeof(int) );        for(j=0; j<nCol; ++j) aNum[i][j] = 0;   }// 결과 출력하기   for(i=0; i<nRow; ++i)   {        for(j=0; j<nCol; ++j) printf("%d", aNum[i][j]);        printf("\n");   }// 할당 해제하기   for(i=0; i<nRow; ++i) free(aNum[i]);   free(aNum);}
