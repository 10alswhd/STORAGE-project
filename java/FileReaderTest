import java.io.*;public class FileReaderTest { public static void main(String[] args) {  // TODO Auto-generated method stub  try  {   FileReader inFile       = new FileReader("D:\\Inputdata.txt");      new FileWriter("D:\\OutputData.txt");      int data;   while((data = inFile.read()) != -1)   {    System.out.println((char)data);          }   inFile.close();  }catch(IOException e){   System.out.println("오류 발생");  }  } }
